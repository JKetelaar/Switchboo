{% extends 'base.html.twig' %}

{% block title %}Quote{% endblock %}

{% block body %}
    <h1>Quote</h1>

    <table class="table table-striped">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ quote.id }}</td>
        </tr>
        <tr>
            <th>Postcode</th>
            <td>{{ quote.postcode }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ quote.email }}</td>
        </tr>
        <tr>
            <th>GasElectricityType</th>
            <td>{{ quote.gasElectricityType }}</td>
        </tr>
        <tr>
            <th>SameSupplier</th>
            <td>{{ quote.sameSupplier ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>EnergySupplier</th>
            <td>{{ quote.energySupplier }}</td>
        </tr>
        <tr>
            <th>Plan</th>
            <td>{{ quote.plan }}</td>
        </tr>
        <tr>
            <th>EconomyMeter</th>
            <td>{{ quote.economyMeter ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>PaymentType</th>
            <td>{{ quote.paymentType }}</td>
        </tr>
        <tr>
            <th>PlanNotSure</th>
            <td>{{ quote.planNotSure ? 'Yes' : 'No' }}</td>
        </tr>
        <tr>
            <th>GasMoneySpend</th>
            <td>{{ quote.gasMoneySpend }}</td>
        </tr>
        <tr>
            <th>GasMoneyPerType</th>
            <td>{{ quote.gasMoneyPerType }}</td>
        </tr>
        <tr>
            <th>GasUseKWH</th>
            <td>{{ quote.gasUseKWH }}</td>
        </tr>
        <tr>
            <th>ElecMoneySpend</th>
            <td>{{ quote.elecMoneySpend }}</td>
        </tr>
        <tr>
            <th>ElecMoneyPerType</th>
            <td>{{ quote.elecMoneyPerType }}</td>
        </tr>
        <tr>
            <th>ElecUseKWH</th>
            <td>{{ quote.elecUseKWH }}</td>
        </tr>
        <tr>
            <th>ChosenSupplier</th>
            <td>{{ quote.chosenSupplier }}</td>
        </tr>
        </tbody>
    </table>

    <a href="{{ path('quote_index') }}">back to list</a>
{% endblock %}
